		"svelte-sonner": "^1.0.5",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.13",
		"tw-animate-css": "^1.3.8",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.0",
		"vite": "^7.1.7",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^0.1.0"
